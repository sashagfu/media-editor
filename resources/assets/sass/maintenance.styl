  .about {
    padding-top : 50px;
  }

  .services.col-md-6 {
    padding-top : 150px;
  }

  .team {
    padding-top : 80px;
  }

  .subscribe button {
    width : 13%;
  }
}

@media only screen and (max-width : 1680px) {

  .about {
    padding-top : 20px;
  }

  .services.col-md-6 {
    padding-top : 130px;
  }

  .team {
    padding-top : 65px;
  }

}

@media only screen and (max-width : 1366px) {

  .about .intro-text {
    padding-top : 130px;
  }

  .services.col-md-6 {
    padding-top : 110px;
  }

  .team {
    padding-top : 45px;
  }

  .team img {
