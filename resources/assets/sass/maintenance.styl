  }

  header p {
    font-size      : 14px;
    color          : #fff;
    text-align     : left;
    width          : 200px;
    padding-bottom : 10px;
  }

}

@media only screen and (-webkit-min-device-pixel-ratio : 2) {
  .twitter.col-md-6, .services-photo, .team-photo, .contact-photo, .subscribe.col-md-6 {
    -webkit-background-size : cover;
    background-position     : center;
  }
}

